{
  "name": "smashing-react-i18n",
  "version": "1.0.0",
  "description": "",
  "main": "dist/bundle.js",
  "scripts": {
    "build:langs": "./node_modules/.bin/babel scripts/translate.js | node",
    "build": "better-npm-run build",
    "webpack-devserver": "webpack-dev-server --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8050 --host 0.0.0.0",
    "nodemon": "better-npm-run nodemon"
  },
  "betterScripts": {
    "build": {
      "command": "webpack -p",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "nodemon": {
      "command": "nodemon server.js",
      "env": {
        "NODE_PATH": "src"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yury-dymov/smashing-react-i18n.git"
  },
  "author": "Yury Dymov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yury-dymov/smashing-react-i18n/issues"
  },
  "homepage": "https://github.com/yury-dymov/smashing-react-i18n#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "clean-webpack-plugin": "^0.1.14",
    "nodemon": "1.8.1",
    "react-hot-loader": "^1.2.9",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "bluebird": "^3.4.7",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-intl": "^2.4.0"
  }
}
